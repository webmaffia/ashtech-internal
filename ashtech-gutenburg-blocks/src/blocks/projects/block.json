{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ashtech/projects",
  "title": "Ashtech Projects",
  "category": "ashtech",
  "icon": "building",
  "description": "Ongoing projects showcase section",
  "keywords": ["projects", "ongoing", "showcase"],
  "textdomain": "ashtech-blocks",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "attributes": {
    "label": {
      "type": "string",
      "default": "Ongoing Projects"
    },
    "title": {
      "type": "string",
      "default": "An Expression of Refinement"
    },
    "description": {
      "type": "string",
      "default": "Discover Ashtech Presidential Towers, a 5.6-acre architectural masterpiece in Greater Noida, where the purest elements of nature meet modern precision to create a living experience that is rare, radiant and profoundly refined."
    },
    "button1Text": {
      "type": "string",
      "default": "Explore More"
    },
    "button2Text": {
      "type": "string",
      "default": "Book a Private Tour"
    },
    "buildingImageUrl": {
      "type": "string",
      "default": ""
    },
    "buildingImageId": {
      "type": "number",
      "default": 0
    }
  },
  "supports": {
    "html": false,
    "align": ["wide", "full"],
    "className": false,
    "customClassName": false
  }
}

